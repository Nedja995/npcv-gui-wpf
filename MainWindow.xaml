<Window x:Class="NPGui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NPGui"
        xmlns:dc="clr-namespace:NPGui.Controls"
        mc:Ignorable="d"
        Title="MainWindow" Height="486" Width="923"
        ResizeMode="CanResize">
    <Grid>
        <TabControl x:Name="imageTabsControl">
            <TabItem Header="imageTab1">
                <Grid Background="#FFE5E5E5" >
                    <dc:ImageWorkspaceControl HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                </Grid>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
